{
  "name": "react-apple-tree",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "copy_files": "copyfiles -u 1 src/public/**/** dist/",
    "build": "npm run clean && tsc && npm run copy_files",
    "watch": "nodemon --exec \"tsc && npm run copy_files\" --watch src --ext ts,tsx,css"
  },
  "dependencies": {
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-window": "^1.8.10"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.3",
    "@types/react-window": "^1.8.8",
    "babel-plugin-lodash": "^3.3.4",
    "copyfiles": "^2.4.1",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "styled-components": "6.1.11",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "src"
  ]
}
