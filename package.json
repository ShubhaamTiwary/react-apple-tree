{
  "name": "@newtonschool/react-apple-tree",
  "version": "0.0.15",
  "description": "React Apple Tree is an updated version of the popular library react-sortable-tree.",
  "keywords": [
    "React Apple Tree",
    "Sortable Tree",
    "React"
  ],
  "homepage": "https://github.com/Newton-School/react-apple-tree",
  "author": "Newton School",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Newton-School/react-apple-tree"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Newton-School/react-apple-tree/issues"
  },
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist/",
    "copy_files": "cp -r src/public dist/",
    "build": "npm run clean && tsc && npm run copy_files",
    "watch": "nodemon --exec \"tsc && npm run copy_files\" --watch src --ext ts,tsx,css",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-window": "^1.8.10"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-flow": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/react": "^18.3.3",
    "@types/react-window": "^1.8.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "styled-components": "6.1.11",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
